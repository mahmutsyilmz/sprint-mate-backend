# Sprint Mate Backend - Environment Variables
# Copy this file to .env and fill in your values

# GitHub OAuth2 Credentials
# Obtain from: GitHub > Settings > Developer settings > OAuth Apps
GITHUB_CLIENT_ID=your-github-client-id
GITHUB_CLIENT_SECRET=your-github-client-secret

# AI Service API Keys
CLARIFAI_API_KEY=your-clarifai-api-key
GROQ_API_KEY=your-groq-api-key

# Database Credentials
DB_URL=jdbc:sqlserver://localhost\\SQLEXPRESS;databaseName=SprintMate;encrypt=true;trustServerCertificate=true;
DB_USERNAME=your-db-username
DB_PASSWORD=your-db-password
